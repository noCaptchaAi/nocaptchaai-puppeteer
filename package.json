{
  "name": "nocaptchaai-puppeteer",
  "version": "2.0.1",
  "description": "Puppeteer package for NoCaptchaAI ⚡️",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "typings": "lib/index.d.ts",
  "scripts": {
    "dev": "ts-node example/index.ts",
    "ts-check": "tsc",
    "clean": "rimraf lib",
    "build": "tsc",
    "release": "pnpm clean && pnpm build && release-it",
    "lint": "eslint . --config .eslintrc.json --cache",
    "prettier": "prettier -c . --config .prettierrc.json",
    "format": "prettier -w . --config .prettierrc.json",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "homepage": "https://nocaptchaai.com",
  "repository": {
    "type": "git",
    "url": "https:git@github.com:noCaptchaAi/nocaptchaai-puppeteer.git"
  },
  "bugs": {
    "url": "https://github.com/noCaptchaAi/nocaptchaai-puppeteer/issues"
  },
  "author": "MohamadKh75",
  "license": "MIT",
  "keywords": [
    "puppeteer",
    "noCaptchaAi",
    "captcha",
    "captcha-solver",
    "captcha-bypass",
    "recaptcha",
    "recaptcha-solver",
    "recaptcha-bypass",
    "hcaptcha",
    "hcaptcha-solver",
    "hcaptcha-bypass"
  ],
  "dependencies": {
    "axios": "^1.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "puppeteer": "^19.8.5",
    "release-it": "^15.10.1",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "puppeteer": "*"
  }
}
